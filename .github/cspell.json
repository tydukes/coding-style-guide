  ],
  "flagWords": [],
  "patterns": [
    {
      "name": "markdown-code-blocks",
      "pattern": "/```[\\s\\S]*?```/g"
    },
    {
      "name": "inline-code",
      "pattern": "/`[^`]*`/g"
    },
    {
      "name": "urls",
      "pattern": "/\\bhttps?://\\S+/gi"
    },
    {
      "name": "email-addresses",
      "pattern": "/\\b[A-Z0-9._%+-]+@[A-Z0-9.-]+\\.[A-Z]{2,}\\b/gi"
    },
    {
      "name": "git-hashes",
      "pattern": "/\\b[0-9a-f]{40}\\b/gi"
    },
    {
      "name": "sha256-hashes",
      "pattern": "/\\b[0-9a-f]{64}\\b/gi"
    }
  ],
  "allowCompoundWords": true,
  "dictionaries": [
    "en_US",
    "companies",
    "softwareTerms",
